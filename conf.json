{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "(node_modules|docs)"
        ],
        "includePattern": ".js$",
        "excludePattern": ""
    },
    "plugins": [
        "jsdoc-vuex-plugin"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "recurse": true,
        "destination":"./docs/"
    }
}